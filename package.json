{
  "name": "sanji-time-ui",
  "author": "Zack Yang <zack9433@gmail.com> (https://github.com/zack9433)",
  "description": "Sanji Time UI",
  "version": "0.0.0-semantic-released",
  "main": "index",
  "config": {
    "ghooks": {
      "pre-commit": ""
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "validate": "npm run test:once && npm run check-coverage",
    "start": "npm run mock:serve && npm run dev:serve",
    "dev:serve": "NODE_ENV=development node server/dev-server.js && kill $npm_package_config_mockserver",
    "test": "NODE_ENV=test BABEL_ENV=test karma start",
    "test:e2e": "npm run mock:serve && npm run protractor",
    "test:once": "BABEL_ENV=test karma start",
    "protractor": "protractor protractor.conf.js && kill $npm_package_config_mockserver",
    "mock:serve": "mockserver -I schema & npm config set sanji-time-ui:mockserver $!",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 75 --branches 75 --functions 75 --lines 75",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "post-install": "webdirver-manager update"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "autoprefixer": "~6.3.7",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.0",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-lodash": "^3.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "~3.5.0",
    "commitizen": "^2.8.0",
    "css-loader": "~0.23.1",
    "cucumber": "~0.7.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.0.0",
    "eslint-loader": "^1.3.0",
    "expose-loader": "~0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "~0.9.0",
    "ghooks": "^1.2.0",
    "html-webpack-plugin": "~2.22.0",
    "istanbul": "~0.4.2",
    "json-loader": "~0.5.4",
    "karma": "~0.13.19",
    "karma-chai-plugins": "~0.7.0",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "~1.1.0",
    "karma-mocha": "~1.1.1",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "~1.7.0",
    "lodash-webpack-plugin": "^0.9.2",
    "mocha": "~2.5.3",
    "ng-annotate-loader": "0.1.0",
    "ng-cache-loader": "0.0.15",
    "node-bourbon": "~4.2.3",
    "node-sass": "~3.8.0",
    "opt-cli": "~1.5.1",
    "postcss-loader": "~0.9.1",
    "protractor": "~2.5.1",
    "raw-loader": "~0.5.1",
    "sanji-openapi-mock": "github:sanji-io/sanji-openapi-mockserver",
    "sass-loader": "~4.0.0",
    "semantic-release": "^4.3.5",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-notifier": "~1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-time-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-Io/sanji-time-ui/issues"
  },
  "keywords": [
    "sanji"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Sanji-IO/sanji-time-ui",
  "peerDependencies": {
    "angular-clock": "~0.5.1"
  },
  "dependencies": {
    "angular-clock": "~0.5.1",
    "sanji-core-ui": "~1.16.0"
  }
}
